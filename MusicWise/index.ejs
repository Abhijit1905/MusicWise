    <div class="reblackbig d-flex flex-column align-items-center gap-5">
      <div class="outrotiles d-flex flex-wrap justify-content-center">
        <div class="outro1 ghostbox megabox">
          <p class="outroboxtext">WE ARE</p>
        </div>
        <div class="outro2 outrobox megabox">
          <p class="outroboxtext">BACKING</p>
        </div>
        <div class="outro3 ghostbox megabox">
