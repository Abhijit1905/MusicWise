  <section class="outrocard d-flex justify-content-center align-items-center">
      <div class="cardbig d-flex flex-column align-items-center justify-content-center ">
        <div class="d-flex flex-column align-items-center justify-content-center gap-5 pt-5">
          <h2 class="outroheadingwhite" style="max-width: 695px; text-align: center; font-weight: 700;">
            Invest with <span class="outroheadingpup" style="color: #00e5a9;">Music Wises </span>instead of marketeers
          </h2>
          <a href="#"><button class="contactus">CONTACT US</button></a>
        </div>
        <div class="backnoise"></div>
        <img src="./images/arrowthin.svg" class="arrowthin">
        <img src="./images/circlefill.svg" class="circle">
        <img src="./images/triangle.svg" class="triangle">
